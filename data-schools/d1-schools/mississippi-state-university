    <script>
    $(function() {
      var athleteButton = $(".lead-form button#athlete-button");
      var parentButton = $(".lead-form button#parent-button");

      function showAthleteOrParentForm(role) {
        var hasUnderThirteenCookie = document.cookie.indexOf("ncsaUserIsUnder13=1") !== -1;

        if (hasUnderThirteenCookie) {
          $("#under-thirteen-modal").css('display', 'flex');
        }

        $(".lead-form #info").hide();
        $(".lead-form #" + role + "-form").show();
      }

      athleteButton.bind("click", function() {
        showAthleteOrParentForm("athlete")
      });

      parentButton.bind("click", function() {
        showAthleteOrParentForm("parent")
      });
    });
  </script>
  <script>
    $(function() {
      var isAthleteForm = true;
      var isBirthDateNeeded = false;
      var baseSelector = isAthleteForm ? "#athlete-form " : "#parent-form "

      var now = new Date();
      var currentYear = now.getUTCFullYear()
      var graduationYearLimit = now.getMonth() + 1 >= 9 ? currentYear + 6 : currentYear + 5;

      var $birthDateInput = $(baseSelector + "#edit-athlete-birth-date")
      var $graduationInput = $(baseSelector + "#edit-athlete-grad-year")

      $birthDateInput.on("keyup", function(e) {
        var v = this.value;
        if (v.match(/^\d{2}$/) !== null) {
          // If we match two consecutive numbers, add a forward slash
          this.value = v + '/';
        } else if (v.match(/^\d{2}\/\d{2}$/) !== null) {
          // If we match two consecutive numbers followed by a slash
          // followed by two conseecutive numbers, add a forward slash
          this.value = v + '/';
        } else {
          // If neither check above matches, remove all non-numbers or forward-slashes
          this.value = v.replace(/[^0-9\/]/g,'');
        }
      })

      $graduationInput.on("change", function(e) {
        var graduationYear = e.target.value;
        isBirthDateNeeded = graduationYear >= graduationYearLimit
        var birthdateFieldWrapper = $(baseSelector + ".birthdate-field-wrapper")

        if (isBirthDateNeeded) {
          birthdateFieldWrapper.show();
        } else {
          birthdateFieldWrapper.hide();
        }
      })

      $.validator.addMethod(
        "validBirthdate",
        function(value) {
          // RegEx for mm/dd/yyyy
          const regex = /^(0[1-9]|1[012])[/.](0[1-9]|[12][0-9]|3[01])[/.](19|20)\d\d$/;
          return regex.test(value);
        },
        "Birthdate must be formatted mm/dd/yyyy."
      );

      $(baseSelector + "form").validate({
        rules: {
          "lead_submission[athlete_first_name]": "required",
          "lead_submission[athlete_last_name]": "required",
          "lead_submission[athlete_email]": isAthleteForm? "required" : "optional",
          "lead_submission[athlete_sport]": "required",
          "lead_submission[birth_date]": {"date": true, "required": false},
          "lead_submission[graduation_year]": "required",
          "lead_submission[athlete_phone]": "required",
          "lead_submission[zip]": "required",
          "lead_submission[parent_first_name]": "required",
          "lead_submission[parent_last_name]": "required",
          "lead_submission[parent_email]": "required",
          "lead_submission[parent_phone]": "required"
        },
        submitHandler: function(form) {

          var rule = isBirthDateNeeded ? "add" : "remove";
          var $birthDateField = $(baseSelector + '[name="lead_submission[birth_date]"]')
          $birthDateField.rules(rule, {
            required: true,
            validBirthdate: true
          });

          if (!$birthDateField.valid()){
            return false;
          }

          var $form = $(form)
          var $birthDateInput = $form.find("#edit-athlete-birth-date");
          var birthDate = new Date($birthDateInput.val());
          var now = new Date();
          var year = now.getUTCFullYear() - 13;
          var month = now.getUTCMonth();
          var day = now.getUTCDate();
          var youngestBirthDate = new Date(year,month,day,23,59,59,999);

          var expiryday = 24*60*60*1000 // one day in milliseconds
          now.setTime(now.getTime() + expiryday);
          var expires = "expires="+ now.toUTCString();

          if (birthDate >= youngestBirthDate) {
            document.cookie = "ncsaUserIsUnder13=1;" + expires;

            $("#under-thirteen-modal").css('display', 'flex');
            return false;
          }
          form.submit();
        },
        errorContainer: $(baseSelector + "#errorContainer")
      });
    });
  </script>
  <script>
    $(function() {
      var isAthleteForm = false;
      var isBirthDateNeeded = false;
      var baseSelector = isAthleteForm ? "#athlete-form " : "#parent-form "

      var now = new Date();
      var currentYear = now.getUTCFullYear()
      var graduationYearLimit = now.getMonth() + 1 >= 9 ? currentYear + 6 : currentYear + 5;

      var $birthDateInput = $(baseSelector + "#edit-athlete-birth-date")
      var $graduationInput = $(baseSelector + "#edit-athlete-grad-year")

      $birthDateInput.on("keyup", function(e) {
        var v = this.value;
        if (v.match(/^\d{2}$/) !== null) {
          // If we match two consecutive numbers, add a forward slash
          this.value = v + '/';
        } else if (v.match(/^\d{2}\/\d{2}$/) !== null) {
          // If we match two consecutive numbers followed by a slash
          // followed by two conseecutive numbers, add a forward slash
          this.value = v + '/';
        } else {
          // If neither check above matches, remove all non-numbers or forward-slashes
          this.value = v.replace(/[^0-9\/]/g,'');
        }
      })

      $graduationInput.on("change", function(e) {
        var graduationYear = e.target.value;
        isBirthDateNeeded = graduationYear >= graduationYearLimit
        var birthdateFieldWrapper = $(baseSelector + ".birthdate-field-wrapper")

        if (isBirthDateNeeded) {
          birthdateFieldWrapper.show();
        } else {
          birthdateFieldWrapper.hide();
        }
      })

      $.validator.addMethod(
        "validBirthdate",
        function(value) {
          // RegEx for mm/dd/yyyy
          const regex = /^(0[1-9]|1[012])[/.](0[1-9]|[12][0-9]|3[01])[/.](19|20)\d\d$/;
          return regex.test(value);
        },
        "Birthdate must be formatted mm/dd/yyyy."
      );

      $(baseSelector + "form").validate({
        rules: {
          "lead_submission[athlete_first_name]": "required",
          "lead_submission[athlete_last_name]": "required",
          "lead_submission[athlete_email]": isAthleteForm? "required" : "optional",
          "lead_submission[athlete_sport]": "required",
          "lead_submission[birth_date]": {"date": true, "required": false},
          "lead_submission[graduation_year]": "required",
          "lead_submission[athlete_phone]": "required",
          "lead_submission[zip]": "required",
          "lead_submission[parent_first_name]": "required",
          "lead_submission[parent_last_name]": "required",
          "lead_submission[parent_email]": "required",
          "lead_submission[parent_phone]": "required"
        },
        submitHandler: function(form) {

          var rule = isBirthDateNeeded ? "add" : "remove";
          var $birthDateField = $(baseSelector + '[name="lead_submission[birth_date]"]')
          $birthDateField.rules(rule, {
            required: true,
            validBirthdate: true
          });

          if (!$birthDateField.valid()){
            return false;
          }

          var $form = $(form)
          var $birthDateInput = $form.find("#edit-athlete-birth-date");
          var birthDate = new Date($birthDateInput.val());
          var now = new Date();
          var year = now.getUTCFullYear() - 13;
          var month = now.getUTCMonth();
          var day = now.getUTCDate();
          var youngestBirthDate = new Date(year,month,day,23,59,59,999);

          var expiryday = 24*60*60*1000 // one day in milliseconds
          now.setTime(now.getTime() + expiryday);
          var expires = "expires="+ now.toUTCString();

          if (birthDate >= youngestBirthDate) {
            document.cookie = "ncsaUserIsUnder13=1;" + expires;

            $("#under-thirteen-modal").css('display', 'flex');
            return false;
          }
          form.submit();
        },
        errorContainer: $(baseSelector + "#errorContainer")
      });
    });
  </script>
  <script>
    $(function() {
      $(".under-thirteen-modal__close-icon").on("click", function() {
        $("#under-thirteen-modal").hide();
        $(".form-select-me-inner").hide();
        $("#under-thirteen-message").show();
      });
    })
  </script>
  <script>
    $(function() {
      var athleteButton = $(".mobile-menu__button--athlete");
      var parentButton = $(".mobile-menu__button--parent");

      function showAthleteOrParentForm(role) {
        var otherRole = role === "athlete" ? "parent" : "athlete";
        var hasUnderThirteenCookie = document.cookie.indexOf("ncsaUserIsUnder13=1") !== -1;

        if (hasUnderThirteenCookie) {
          $("#under-thirteen-modal").css('display', 'flex');
        }

        $(".lead-form #info").hide();
        $(".lead-form #" + otherRole + "-form").hide();
        $(".lead-form #" + role + "-form").show();
      }

      athleteButton.bind("click", function(e) {
        e.preventDefault()
        showAthleteOrParentForm("athlete");
        // Scroll into view
        document.getElementById("athlete-form").scrollIntoView();
      });

      parentButton.bind("click", function(e) {
        e.preventDefault()
        showAthleteOrParentForm("parent");
        // Scroll into view
        document.getElementById("parent-form").scrollIntoView();
      });
    });
  </script>
